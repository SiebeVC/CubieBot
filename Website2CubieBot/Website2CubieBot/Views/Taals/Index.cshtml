@model IEnumerable<Website2CubieBot.Models.Taal>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Keuze)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Inhoud)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Volgorde)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Locatie)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Keuze)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Inhoud)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Volgorde)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Locatie)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
